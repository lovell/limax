{
  "name": "limax",
  "version": "4.1.0",
  "main": "./lib/limax.cjs",
  "types": "./lib/limax.d.ts",
  "type": "module",
  "description": "Node.js module to generate URL slugs. Another one? This one cares about i18n and transliterates non-Latin scripts to conform to the RFC3986 standard. Mostly API-compatible with similar modules.",
  "repository": {
    "type": "git",
    "url": "git://github.com/lovell/limax"
  },
  "engines": {
    "node": ">=14"
  },
  "files": [
    "CHANGELOG.md",
    "index.d.ts",
    "lib/"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./lib/limax.d.mts",
        "default": "./lib/limax.mjs"
      },
      "require": {
        "types": "./lib/limax.d.cts",
        "default": "./lib/limax.cjs"
      }
    },
    "./cjs": {
      "types": "./lib/limax.d.cts",
      "default": "./lib/limax.cjs"
    },
    "./esm": {
      "types": "./lib/limax.d.mts",
      "default": "./lib/limax.mjs"
    }
  },
  "scripts": {
    "test": "semistandard && nyc --reporter=html ava",
    "changelog": "conventional-changelog -i CHANGELOG.md -s",
    "build": "unbuild",
    "prepack": "unbuild"
  },
  "keywords": [
    "slug",
    "url",
    "clean",
    "speaking",
    "seo",
    "i18n",
    "l10n",
    "transliterate",
    "script",
    "language",
    "pinyin",
    "hepburn"
  ],
  "author": "Lovell Fuller <npm@lovell.info>",
  "contributors": [
    "Zhe Feng <fengzhe1983@gmail.com>",
    "Aleksey Olenyuk <alo@qapint.com>",
    "Daryl Chan <dvrylc@gmail.com>",
    "Arjan Frans <arjanfrans.com@gmail.com>",
    "Fabrice Labbe <fabrice.labbe@adfab.fr>",
    "Lukas Spie√ü <lukas.spiess@gmail.com>",
    "David Dijan <david.djian@traceweb.fr>",
    "Benjamin Piouffle <benjamin@opencollective.com>"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "hepburn": "^1.2.0",
    "lodash.deburr": "^4.1.0",
    "speakingurl": "^14.0.1",
    "pinyin-pro": "^3.14.0"
  },
  "devDependencies": {
    "ava": "^5.2.0",
    "conventional-changelog-cli": "^2.2.2",
    "nyc": "^15.1.0",
    "semistandard": "^16.0.1",
    "unbuild": "^3.6.0"
  }
}